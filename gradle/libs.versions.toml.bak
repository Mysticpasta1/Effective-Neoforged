[versions]
# The latest versions are available at https://lambdaurora.dev/tools/import_quilt.html
minecraft = "1.20.1"
forge = "1.20.1-47.2.32"


# Third party dependencies
# The latest versions should be available at https://ladysnake.org/tools/dependencies
iris = "1.6.11+1.20.1"
lodestoneLib = "5365395"
midnightLib = "4980331"
satin = "5299671"
sodium = "5410220"
jcpp = "1.4.14"
glslTransformer = "2.0.0-pre13"

[libraries]
minecraft = { module = "com.mojang:minecraft", version.ref = "minecraft" }
forge = { module = "net.minecraftforge:forge", version.ref = "forge" }

# Third party dependencies
iris = { module = "curse.maven:oculus-581495", version.ref = "iris" }
jcpp = { module = "org.anarres:jcpp", version.ref = "jcpp" }
glslTransformer = { module = "io.github.douira:glsl-transformer", version.ref = "glslTransformer" }
lodestoneLib = { module = "curse.maven:lodestone-616457", version.ref = "lodestoneLib" }
midnightLib = { module = "curse.maven:midnightlib-488090", version.ref = "midnightLib" }
satin = { module = "org.ladysnake:satin", version.ref = "satin" }
sodium = { module = "curse.maven:embeddium-908741", version.ref = "sodium"}

[plugins]
forge-loom = { id = "dev.architectury.loom", version = "1.6-SNAPSHOT" }
